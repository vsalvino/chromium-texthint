{
    "name": "Text Hinting",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Enhances text rendering to approximate system ClearType. Free and Open Source.",
    "homepage_url": "https://github.com/vsalvino/chromium-texthint",
    "icons": {
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["text.css"]
        }
    ]
}
